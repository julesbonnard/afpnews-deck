{
  "name": "afpnews-deck",
  "version": "0.3.5-alpha",
  "description": "My dream reader for AFP feeds",
  "homepage": "https://github.com/julesbonnard/afpnews-deck",
  "repository": {
    "type": "git",
    "url": "git@github.com:julesbonnard/afpnews-deck.git"
  },
  "author": "Jules Bonnard",
  "main": "dist/afpnews-deck.electron.js",
  "build": {
    "productName": "afpNewsDeck",
    "appId": "com.afpnews.deck",
    "mac": {
      "artifactName": "${productName}-${version}-osx.${ext}",
      "category": "public.news.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "artifactName": "${productName}-${version}-${arch}-linux.${ext}",
      "maintainer": "Jules Bonnard",
      "target": [
        "tar.gz",
        "deb",
        "rpm"
      ]
    },
    "win": {
      "target": [
        "zip",
        "dir"
      ]
    },
    "files": [
      "dist/**/*",
      "static/icons/**/*"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development --env.NODE_ENV=development --host 0.0.0.0",
    "build": "BABEL_ENV=production webpack --mode production --env.NODE_ENV=production",
    "electron": "electron dist/afpnews-deck.electron.js",
    "lint": "eslint --ext .js,.vue src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -m",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@afp/toolkit-styles": "^1.3.3",
    "afpnews-api": "^1.2.2",
    "autoprefixer": "^8.6.3",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-window-state": "^4.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "localforage": "^1.7.2",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url": "^0.11.0",
    "url-loader": "^1.0.1",
    "uuid": "^3.2.1",
    "vue": "^2.5.16",
    "vue-clickaway": "^2.2.2",
    "vue-linkify": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vue2-hammer": "^1.0.6",
    "vuejs-datepicker": "^1.4.0",
    "vuex": "^3.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "workbox-webpack-plugin": "^3.3.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
