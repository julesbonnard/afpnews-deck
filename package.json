    "daynight": "^2.0.8",
    "fontfaceobserver": "^2.1.0",
    "hammerjs": "^2.0.8",
    "localforage": "^1.7.3",
    "localforage-getitems": "^1.4.2",
    "localforage-setitems": "^1.4.0",
    "lucene": "^2.1.1",
    "normalize.css": "^8.0.1",
