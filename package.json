    "fontfaceobserver": "^2.1.0",
    "hammerjs": "^2.0.8",
    "localforage": "^1.7.3",
    "lucene": "^2.0.3",
    "modernizr": "^3.7.1",
    "normalize.css": "^8.0.1",
