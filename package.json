    "d3-zoom": "^1.8.3",
    "dayjs": "^1.8.16",
    "daynight": "^2.0.7",
    "fontfaceobserver": "^2.1.0",
    "hammerjs": "^2.0.8",
    "localforage": "^1.7.3",
    "localforage-getitems": "^1.4.2",
    "localforage-setitems": "^1.4.0",
    "lucene": "^2.1.0",
    "modernizr": "^3.7.1",
