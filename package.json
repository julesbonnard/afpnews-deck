{
  "name": "afpnews-deck",
  "version": "1.0.0-beta",
  "description": "My dream reader for AFP feeds",
  "homepage": "https://github.com/julesbonnard/afpnews-deck",
  "repository": {
    "type": "git",
    "url": "git@github.com:julesbonnard/afpnews-deck.git"
  },
  "author": "Jules Bonnard",
  "build": {
    "productName": "afpNewsDeck",
    "appId": "com.afpnews.deck"
  },
  "main": "dist_electron/bundled/background.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:app && npm run build:sw",
    "build:app": "vue-cli-service build --modern",
    "build:sw": "webpack --mode production dist/service-worker.js --resolve-alias @=../src --target webworker -o dist/sw.min.js",
    "now-build": "npm run build"
  },
  "dependencies": {
    "@afp/toolkit-styles": "^2.0.0",
    "afpnews-api": "^1.5.1",
    "d3-ease": "^1.0.5",
    "d3-selection": "^1.3.2",
    "d3-transition": "^1.1.3",
    "d3-zoom": "^1.7.3",
    "dayjs": "^1.7.8",
    "fontfaceobserver": "^2.1.0",
    "hammerjs": "^2.0.8",
    "intro.js": "^2.9.3",
    "localforage": "^1.7.3",
    "lucene-query-parser": "^1.2.0",
    "modernizr": "^3.6.0",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.5.2",
    "url": "^0.11.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.21",
    "vue-analytics": "^5.16.1",
    "vue-clickaway": "^2.2.2",
    "vue-content-placeholders": "^0.2.1",
    "vue-highlight-words": "^0.1.3",
    "vue-i18n": "^8.4.0",
    "vue-linkify": "^1.0.1",
    "vue-meta": "^1.5.6",
    "vue-router": "^3.0.2",
    "vue-wait": "^1.3.3",
    "vue2-hammer": "^2.1.2",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-plugin-pwa": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "json-loader": "^0.5.7",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-template-compiler": "^2.5.21",
    "webpack-cli": "^3.1.2"
  }
}
